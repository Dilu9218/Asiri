                           </children>
                        </VBox>
                        <VBox alignment="CENTER" prefWidth="70.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#add" text="Add" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" prefWidth="676.0">
                     <children>
                        <TableView fx:id="productTable" prefHeight="250.0" prefWidth="590.0">
