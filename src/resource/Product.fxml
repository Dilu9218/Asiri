                              <TableColumn fx:id="idC" prefWidth="105.0" text="ID" />
                              <TableColumn fx:id="nameC" prefWidth="210.0" text="Name" />
                              <TableColumn fx:id="brandC" prefWidth="160.0" text="Brand" />
                              <TableColumn fx:id="strengthC" prefWidth="60.0" text="Strength" />
                              <TableColumn fx:id="typeC" prefWidth="55.0" text="Type" />
                           </columns>
                        </TableView>
                        <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="85.0" spacing="20.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#edit" prefWidth="50.0" text="Edit" />
                              <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#update" text="Update" />
                              <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
                           </children>
                           <padding>
                              <Insets top="25.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="179.0" prefWidth="705.0">
               <children>
                  <HBox alignment="CENTER_RIGHT" prefWidth="676.0" spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#clear" prefWidth="51.0" text="Clear" />
