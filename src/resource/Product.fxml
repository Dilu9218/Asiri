<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ProductController">
    <stylesheets>
        <URL value="@product.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" prefHeight="580.0" prefWidth="915.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <VBox alignment="CENTER" spacing="5.0">
                           <children>
                              <Label text="ID" />
                              <TextField fx:id="id" prefWidth="100.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="236.0" layoutY="10.0" spacing="5.0">
                           <children>
                              <Label text="Name" />
                              <TextField fx:id="name" prefWidth="200.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="395.0" layoutY="10.0" spacing="5.0">
                           <children>
                              <Label text="Brand" />
                              <TextField fx:id="brand" prefWidth="130.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="467.0" layoutY="10.0" spacing="5.0">
                           <children>
                              <Label text="Strength" />
                              <TextField fx:id="strength" prefWidth="70.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="498.0" layoutY="10.0" spacing="5.0">
                           <children>
                              <Label text="Type" />
                              <ComboBox fx:id="type" prefWidth="70.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="550.0" layoutY="10.0" spacing="5.0">
                           <children>
                              <Label />
                              <Button mnemonicParsing="false" onAction="#search" prefWidth="60.0" text="Search" />
                           </children>
                           <padding>
                              <Insets left="25.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefWidth="705.0" spacing="10.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefWidth="676.0" spacing="10.0">
                     <children>
                        <VBox alignment="TOP_CENTER" spacing="5.0">
                           <children>
                              <Label alignment="CENTER" prefWidth="100.0" text="Description" />
                              <TextArea fx:id="description" prefHeight="50.0" prefWidth="450.0" />
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </VBox>
                        <VBox alignment="CENTER" spacing="5.0">
                           <children>
                               <Label alignment="CENTER" prefWidth="100.0" text="Min Stock" />
